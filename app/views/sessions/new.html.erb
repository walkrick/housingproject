<h1 Login</h1>

<div class='errors flash'><%= flash[:error] if flash[:error] %></div>

<%= form_for User.new, url: sessions_path do |f| %>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.submit "Login!" %>

<% end %>