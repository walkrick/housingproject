
<h3>Login</h3>

<%= form_for User.new, url: sessions_path do |f| %>

  <div><%= flash[:error] if flash[:error] %></div>

  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password_digest %>
  <br>
  <%= f.submit "Login" %>

<% end %>